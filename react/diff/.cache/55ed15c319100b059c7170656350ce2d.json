ct-dom\\render.js","original":{"line":77,"column":4}},{"generated":{"line":94,"column":0},"source":"src\\react-dom\\render.js","original":{"line":78,"column":4}},{"name":"container","generated":{"line":94,"column":11},"source":"src\\react-dom\\render.js","original":{"line":78,"column":11}},{"name":"appendChild","generated":{"line":94,"column":21},"source":"src\\react-dom\\render.js","original":{"line":78,"column":21}},{"generated":{"line":94,"column":32},"source":"src\\react-dom\\render.js","original":{"line":78,"column":11}},{"name":"_render","generated":{"line":94,"column":33},"source":"src\\react-dom\\render.js","original":{"line":78,"column":33}},{"name":"vnode","generated":{"line":94,"column":41},"source":"src\\react-dom\\render.js","original":{"line":78,"column":41}},{"generated":{"line":94,"column":46},"source":"src\\react-dom\\render.js","original":{"line":78,"column":33}},{"generated":{"line":94,"column":47},"source":"src\\react-dom\\render.js","original":{"line":78,"column":11}},{"generated":{"line":94,"column":48},"source":"src\\react-dom\\render.js","original":{"line":78,"column":4}},{"generated":{"line":95,"column":0},"source":"src\\react-dom\\render.js","original":{"line":79,"column":1}}],"sources":{"src\\react-dom\\render.js":"import { setAttribute } from './dom.js';\r\nimport Component from '../react/component.js';\r\n/**\r\n * 将虚拟DOM变真实DOM\r\n * @params vnode 虚拟DOM\r\n * @return 返回DOM\r\n */\r\nfunction _render(vnode) {\r\n    // console.log(vnode);\r\n    // 1. 递归 将结点转成DOM 子结点递归 出口就是文本结点\r\n    // 2. 结点类型 三种情况：文本结点 return createTextNode() 退出\r\n    // 标签结点 createElement attrs children递归_render\r\n    // component render(return jsx)\r\n    // return document.createTextNode('render');\r\n    if(vnode === undefined || vnode === null || typeof vnode === 'boolean') {\r\n        vnode = '';\r\n    }\r\n    if(typeof vnode === 'number') {\r\n        // console.log(vnode);\r\n        vnode = String(vnode);\r\n    }\r\n    if(typeof vnode === 'string') {\r\n        let textNode = document.createTextNode(vnode);\r\n        return textNode;\r\n    }\r\n    // <Counter /> 不是正常标签 vnode.tag = function Counter(){}\r\n    if(typeof vnode.tag === 'function') {\r\n        // console.log(vnode);\r\n        // return document.createTextNode('component');\r\n        const component = createComponent(vnode.tag, vnode.attrs);\r\n        setComponentProps(component, vnode.attrs);\r\n        return component.base;\r\n    }\r\n    const dom = document.createElement(vnode.tag);\r\n    if(vnode.attrs) {\r\n        Object.keys(vnode.attrs).forEach(key => {\r\n            const value = vnode.attrs[key];\r\n            setAttribute(dom, key, value);\r\n        })\r\n    }\r\n\r\n    if(vnode.children) {\r\n        vnode.children.forEach(child => render(child, dom));\r\n    }\r\n    return dom;\r\n}\r\nfunction setComponentProps (component, props) {\r\n    component.props = props;\r\n    renderComponent(component);\r\n}\r\n// 将component里的jsx转为DOM 还会在setState时调用\r\nexport function renderComponent (component) {\r\n    let base;//jsx => dom\r\n    const renderer = component.render();\r\n    base = _render(renderer);\r\n    // 非第一次渲染组件\r\n    if(component.base && component.base.parentNode) {\r\n        component.base.parentNode.replaceChild(base, component.base);\r\n    }\r\n    component.base = base;\r\n    base._component = component;\r\n}\r\nfunction createComponent (component, props) {\r\n    let inst;\r\n    if(component.prototype && component.prototype.render) {\r\n        inst = new component(props);\r\n    } else {\r\n        inst = new Component(props);\r\n        inst.constructor = component;\r\n        inst.render = function () {\r\n            return this.constructor(props);\r\n        }\r\n    }\r\n    return inst;\r\n}\r\nexport function render (vnode, container) {\r\n    // console.log(vnode, container);\r\n    return container.appendChild(_render(vnode));\r\n}"},"lineCount":null}},"hash":"7419237db2cc726ea0a67f9ef9f9e341","cacheData":{"env":{}}}